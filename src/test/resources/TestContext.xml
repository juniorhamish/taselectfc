<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="fixtureDAO" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.taselectfc.dao.FixtureDAO" />
    </bean>

    <bean id="idGenerator" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.springframework.util.IdGenerator" />
    </bean>
</beans>
